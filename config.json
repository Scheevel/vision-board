{
  "development": {
    "sass": {
      "outputStyle": "nested",
      "sassDir": "styles/",
      "sourcemap": true
    },
    "browserify": {
      "debug": true,
      "standalone": "ShiftingTiles",
      "transform": ["babelify"]
    },
    "directory": "example/dist"
  },
  "production": {
    "sass": {
      "outputStyle": "compressed",
      "sassDir": "styles/",
      "sourcemap": false
    },
    "browserify": {
      "debug": false,
      "standalone": "ShiftingTiles",
      "transform": ["babelify"]
    },
    "directory": "dist"
  }
}
